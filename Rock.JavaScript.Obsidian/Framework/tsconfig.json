{
  "compilerOptions": {
    "outDir": "../dist/Framework",
    "rootDir": ".",
    "declaration": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "module": "System",
    "moduleResolution": "Node",
    "baseUrl": "../",
    "strict": true,
    "lib": [
      "DOM",
      "ES2020",
      "ES2020.Symbol.WellKnown"
    ],
    "allowUnreachableCode": false
  },
  "exclude": [
    "./Tests/**/*"
  ]
}
